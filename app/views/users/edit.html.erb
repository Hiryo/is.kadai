<%= form_with(model: @user, local: true, url: url_choose, method: method_choose) do |f| %>
    <h1><%= @user.name %>'s page</h1>
    <p>Mail address: <%= @user.email %></p>
    <%= f.file_field :image %>
    <%= f.hidden_field :image_cache %>
    <%= f.submit "Upload" %>
<% end %>
<%= link_to 'Back', user_path %>
